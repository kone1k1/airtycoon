<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="595.0" prefWidth="904.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MenueController">
    <stylesheets>
        <URL value="@menue.css" />
    </stylesheets>
   <children>
      <Pane prefHeight="595.0" prefWidth="904.0">
         <children>
            <VBox layoutX="47.0" layoutY="90.0" prefHeight="382.0" prefWidth="274.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#newGame" prefHeight="37.0" prefWidth="284.0" text="Neues Spiel">
                     <VBox.margin>
                        <Insets bottom="50.0" />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#loadGame" prefHeight="37.0" prefWidth="284.0" text="Spiel Laden">
                     <VBox.margin>
                        <Insets bottom="50.0" />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#exitProg" prefHeight="37.0" prefWidth="284.0" text="Spiel Beenden">
                     <VBox.margin>
                        <Insets bottom="50.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <VBox layoutX="359.0" layoutY="90.0" prefHeight="488.0" prefWidth="510.0">
               <children>
                  <TextField fx:id="txtPlayername" alignment="CENTER" prefHeight="37.0" prefWidth="274.0" promptText="Name">
                     <VBox.margin>
                        <Insets bottom="50.0" />
                     </VBox.margin>
                  </TextField>
                  <ListView fx:id="lstSaves" prefHeight="331.0" prefWidth="351.0" />
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
